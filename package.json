{
  "name": "mojaloop-payment-manager-experience-api-service",
  "version": "3.0.1",
  "description": "Mojaloop Payment Manager - Experience API Service",
  "main": "index.js",
  "author": "Murthy kakarlamudi, ModusBox Inc.",
  "contributors": [
    "Murthy Kakarlamudi <murthy@modusbox.com>",
    "Yevhen Kyriukha <yevhen.kyriukha@modusbox.com>",
    "James Bush <james.bush@modusbox.com>",
    "Juan Correa <juan.correa@modusbox.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pm4ml/mojaloop-payment-manager-experience-api.git"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "node -r dotenv/config src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --ci --reporters=default --reporters=jest-junit --env=node test/unit",
    "test:coverage": "npx nyc --reporter=lcov --reporter=text-summary npm run test",
    "test:coverage-check": "npm run test:coverage && nyc check-coverage",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -e 2 -u",
    "audit:check": "npm audit --audit-level=high",
    "test:xunit": "jest --ci --reporters=jest-junit --env=node",
    "test:integration": "jest --ci --reporters=default --reporters=jest-junit --env=node test/integration --forceExit",
    "test:functional": "jest --ci --reporters=default --reporters=jest-junit --env=node test/functional --forceExit"
  },
  "dependencies": {
    "@internal/accesscontrol": "file:src/lib/accesscontrol",
    "@internal/model": "file:src/lib/model",
    "@internal/randomphrase": "file:src/lib/randomphrase",
    "@internal/requests": "file:src/lib/requests",
    "@internal/utils": "file:src/lib/utils",
    "@koa/cors": "^5.0.0",
    "@mojaloop/sdk-standard-components": "^19.6.2",
    "ajv": "^8.17.1",
    "better-sqlite3": "^11.7.0",
    "co-body": "^6.2.0",
    "dotenv": "^16.4.7",
    "env-var": "^7.5.0",
    "faker": "^6.6.6",
    "knex": "^3.1.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-oas3": "^3.0.1",
    "koa-router": "^13.0.1",
    "koa-session": "^6.4.0",
    "koa2-oauth-server": "^1.0.0",
    "openid-client": "^6.1.7",
    "redis": "^4.7.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/jest": "^29.5.14",
    "codecov": "^3.8.3",
    "eslint": "^9.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nock": "^13.5.6",
    "openapi-response-validator": "^12.1.3",
    "redis-mock": "^0.56.3",
    "supertest": "^7.0.0"
  }
}
